{
	"lazyCodeLoading": "requiredComponents",
	"easycom": {
		"autoscan": true,
		"custom": {
			"fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue",
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/login/quickLogin"
		},
		{
			"path": "pages/index/index"
		},
		{
			"path": "pages/user/index"
		},
		{
			"path": "pages/welcome/index"
		},
		{
			"path": "pages/order/index"
		}, {
			"path": "pages/purchase/purchase",
			"style": {
				"navigationBarTitleText": "洗笑颜开",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/order/pickupAddress",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}

		, {
			"path": "pages/order/doorToDoor",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/order/mailingAddress",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}
	],
	"subpackages": [{
			"root": "packageOrder",
			"pages": [{
					"path": "pages/detail"
				}, {
					"path": "pages/types"
				},
				{
					"path": "pages/doorToDoor"
				}, {
					"path": "pages/mailingAddress"
				}, {
					"path": "pages/pickupAddress"
				}
			]
		},
		{
			"root": "packageUser",
			"pages": [{
					"path": "pages/order/detail"
				}, {
					"path": "pages/order/feedback"
				}, {
					"path": "pages/settings/address"
				}, {
					"path": "pages/settings/index"
				}, {
					"path": "pages/cooperate"
				}, {
					"path": "pages/credit"
				}, {
					"path": "pages/feedback"
				}, {
					"path": "pages/joinUs"
				}, {
					"path": "pages/recycle"
				}, {
					"path": "pages/service",
					"style": {
						"navigationBarTitleText": "洗笑颜开",
						"app-plus": {
							"animationType": "fade-in",
							"background": "transparent",
							"backgroundColor": "transparent",
							"popGesture": "none"
						}
					}
				}, {
					"path": "pages/card",
					"style": {
						"navigationBarTitleText": "",
						"enablePullDownRefresh": false
					}
				},
				{
					"path": "pages/chargeMoney",
					"style": {
						"navigationBarTitleText": "",
						"enablePullDownRefresh": false
					}
				}
			]
		}
	],
	"preloadRule": {
		"pages/order/index": {
			"network": "all",
			"packages": ["packageOrder"]
		},
		"pages/user/index": {
			"network": "all",
			"packages": ["packageUser"]
		}
	},
	"tabBar": {
		"color": "#7a7f82",
		"selectedColor": "#7a7f82",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页",
			"iconPath": "static/tabbar/first.png",
			"selectedIconPath": "static/tabbar/first.png"
		}, {
			"pagePath": "pages/order/index",
			"text": "订单",
			"iconPath": "static/tabbar/second.png",
			"selectedIconPath": "static/tabbar/second.png"
		}, {
			"pagePath": "pages/user/index",
			"text": "我的",
			"iconPath": "static/tabbar/third.png",
			"selectedIconPath": "static/tabbar/third.png"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "洗笑颜开",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
	}
}